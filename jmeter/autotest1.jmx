int valN   = Integer.parseInt(props.get(&quot;split.count.val.${proj}&quot;)   ?: &quot;0&quot;)
int testN  = Integer.parseInt(props.get(&quot;split.count.test.${proj}&quot;)  ?: &quot;0&quot;)

final String cfgRoot = vars.get(&apos;config_path&apos;)

// 选择模板文件名
String tplName
switch (aug) {
  case &apos;small&apos;:
    tplName = &apos;small.json&apos;; break
  case &apos;middle&apos;:
    tplName = &apos;middle.json&apos;; break
  case &apos;large&apos;:
  default:
    tplName = &apos;large.json&apos;
}

File tplFile = new File(cfgRoot + File.separator + &apos;params&apos; + File.separator + tplName)

def slurper = new JsonSlurper()
def payload = slurper.parse(tplFile) as Map

// --- 确保 training_job_create 存在 ---
if (!(payload.training_job_create instanceof Map)) {
    payload.training_job_create = [:]
}
def tj = payload.training_job_create as Map

// --- 确保 preprocessing_steps 容器存在 ---
if (!(tj.preprocessing_steps instanceof Map)) {
    tj.preprocessing_steps = [:]
}
def pp = tj.preprocessing_steps as Map

// 1) 根据 preprocess 切换开关（只改 ENABLE，不增删任何键）
if (pp.RESIZE instanceof Map) {
    pp.RESIZE.ENABLE = (preprocess == &apos;resize&apos;)
}
if (pp.HIRES_SLICE instanceof Map) {
    pp.HIRES_SLICE.ENABLE = (preprocess == &apos;slice&apos;)
}


// 2) 覆盖 split_sizes 数值（保持键名/层级完全不变）
tj.split_sizes = (tj.split_sizes instanceof Map ? tj.split_sizes : [:]) as Map
def ss = tj.split_sizes as Map
