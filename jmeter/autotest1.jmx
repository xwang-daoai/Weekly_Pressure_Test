                    <stringProp name="script">// Language: groovy
import groovy.json.*

// 取循环变量
final String proj       = vars.get(&apos;current_proj_uid&apos;)
final String mode       = (vars.get(&apos;current_mode&apos;) ?: &apos;super_fast&apos;)
final String preprocess = (vars.get(&apos;current_preprocess&apos;) ?: &apos;resize&apos;)   // &apos;resize&apos; or &apos;slice&apos;
final String aug        = (vars.get(&apos;current_aug&apos;) ?: &apos;large&apos;)           // &apos;large&apos; or &apos;small&apos;


if (!proj) throw new IllegalStateException(&quot;current_proj_uid 为空，请检查 FE projects 的 Output variable name&quot;)

// ===== 读取分割数量（已在 FE projects 层计算写入 props）=====
int trainN = Integer.parseInt(props.get(&quot;split.count.train.${proj}&quot;) ?: &quot;0&quot;)
int valN   = Integer.parseInt(props.get(&quot;split.count.val.${proj}&quot;)   ?: &quot;0&quot;)
int testN  = Integer.parseInt(props.get(&quot;split.count.test.${proj}&quot;)  ?: &quot;0&quot;)

final String cfgRoot = vars.get(&apos;config_path&apos;)

// 选择模板文件名
String tplName
switch (aug) {
  case &apos;small&apos;:
    tplName = &apos;small.json&apos;; break
  case &apos;middle&apos;:
    tplName = &apos;middle.json&apos;; break
  case &apos;large&apos;:
  default:
    tplName = &apos;large.json&apos;
}

File tplFile = new File(cfgRoot + File.separator + &apos;params&apos; + File.separator + tplName)

def slurper = new JsonSlurper()
def payload = slurper.parse(tplFile) as Map

// --- 确保 training_job_create 存在 ---
if (!(payload.training_job_create instanceof Map)) {
    payload.training_job_create = [:]
}
def tj = payload.training_job_create as Map

// --- 确保 preprocessing_steps 容器存在 ---
if (!(tj.preprocessing_steps instanceof Map)) {
    tj.preprocessing_steps = [:]
}
def pp = tj.preprocessing_steps as Map

// 1) 根据 preprocess 切换开关（只改 ENABLE，不增删任何键）
if (pp.RESIZE instanceof Map) {
    pp.RESIZE.ENABLE = (preprocess == &apos;resize&apos;)
}
if (pp.HIRES_SLICE instanceof Map) {
    pp.HIRES_SLICE.ENABLE = (preprocess == &apos;slice&apos;)
}


// 2) 覆盖 split_sizes 数值（保持键名/层级完全不变）
tj.split_sizes = (tj.split_sizes instanceof Map ? tj.split_sizes : [:]) as Map
def ss = tj.split_sizes as Map
ss.train       = trainN
ss.validation  = valN
ss.test        = testN


// 3) 顶层 project_uid（按你的后端要求）
payload.project_uid = proj

// 写入 model_type（注意：写在 training_job_create 里）
tj.model_type = mode

// 方便排查：改一个可读的 name（也在 training_job_create 里）
def stamp = new Date().format(&quot;yyMMdd-HHmmss&quot;)
tj.name = &quot;JM-IS ${mode}-${preprocess}-${aug} ${stamp}&quot;

// 顶层 project_uid（如果你的接口 body 需要）
payload.project_uid = proj

// 输出到变量，供 HTTP Sampler 的 Body Data 使用： ${payload}
def json = JsonOutput.prettyPrint(JsonOutput.toJson(payload))
vars.put(&apos;payload&apos;, json)

// —— 调试日志 —— 
log.info(&quot;[JM-PAYLOAD] proj=${proj}, mode=${mode}, preprocess=${preprocess}, aug=${aug}&quot;)
log.info(&quot;[JM-PAYLOAD] name=${tj.name}&quot;)
def totalStr = props.get(&quot;image_count.${proj}&quot;) ?: &quot;0&quot;
int totalAll = (totalStr.isInteger() ? totalStr.toInteger() : 0)
if (trainN + valN + testN != totalAll) {
    log.warn(&quot;[PAYLOAD] split_sizes sum != total for ${proj}: ${trainN}+${valN}+${testN} vs ${totalAll}&quot;)
}
log.info(&quot;[PAYLOAD] proj=${proj}, mode=${mode}, preprocess=${preprocess}, aug=${aug}, split=${trainN}/${valN}/${testN}&quot;)
log.debug(json)

