// —— 调试日志 —— 
log.info(&quot;[JM-PAYLOAD] proj=${proj}, mode=${mode}, preprocess=${preprocess}, aug=${aug}&quot;)
log.info(&quot;[JM-PAYLOAD] name=${tj.name}&quot;)
def totalStr = props.get(&quot;image_count.${proj}&quot;) ?: &quot;0&quot;
int totalAll = (totalStr.isInteger() ? totalStr.toInteger() : 0)
if (trainN + valN + testN != totalAll) {
    log.warn(&quot;[PAYLOAD] split_sizes sum != total for ${proj}: ${trainN}+${valN}+${testN} vs ${totalAll}&quot;)
}
log.info(&quot;[PAYLOAD] proj=${proj}, mode=${mode}, preprocess=${preprocess}, aug=${aug}, split=${trainN}/${valN}/${testN}&quot;)
log.debug(json)

