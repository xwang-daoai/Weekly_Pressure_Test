            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// === inputs ===
// 1) ForEach 的 Output variable name：current_proj_uid
// 2) props 里已有 image_count.&lt;uid&gt;
// 3) UDV: split.train.pct / split.val.pct / split.test.pct  (如 70 或 70%)
def uid = vars.get(&apos;current_proj_uid&apos;)
if (!uid) throw new IllegalStateException(&quot;current_proj_uid 未取到（检查 ForEach 的 Output variable name）&quot;)

// 取总图数
int total = 0
def _t = props.get(&quot;image_count.${uid}&quot;)
if (_t) { try { total = Integer.parseInt(_t) } catch(ignore) {} }

// 读 UDV 百分比（严格版：未配置就报错）
def readPct = { k -&gt;
  def v = (vars.get(k) ?: props.get(k) ?: &apos;&apos;).toString().trim()
  if (!v) throw new IllegalStateException(&quot;缺少 UDV: ${k}&quot;)
  v = v.endsWith(&apos;%&apos;) ? v[0..-2] : v
  Double.parseDouble(v)
}
double pT = readPct(&apos;split.train.pct&apos;)
double pV = readPct(&apos;split.val.pct&apos;)
double pE = readPct(&apos;split.test.pct&apos;)

// 归一化
double sum = pT + pV + pE
if (sum &lt;= 0) throw new IllegalStateException(&quot;split 百分比总和&lt;=0&quot;)
pT/=sum; pV/=sum; pE/=sum

int trainN, valN, testN
if (total &lt; 3) {
  // 极小样本兜底：先填 train，再 val，再 test
  int[] t = [0,0,0]
  for (int i=0;i&lt;total;i++) t[i]++
  (trainN,valN,testN) = [t[0],t[1],t[2]]
} else {
  def exact = [total*pT, total*pV, total*pE]
  def base  = exact.collect { (int)Math.floor(it) }
  int rem = total - base.sum()
  def order = [0,1,2].sort{ -(exact[it]-base[it]) }  // 最大余数法
  for (int i=0;i&lt;rem;i++) base[ order[i%3] ]++
  (trainN,valN,testN) = base
}

// 跨后续所有嵌套循环可用
props.put(&quot;split.count.train.${uid}&quot;, String.valueOf(trainN))
props.put(&quot;split.count.val.${uid}&quot;,   String.valueOf(valN))
props.put(&quot;split.count.test.${uid}&quot;,  String.valueOf(testN))
props.put(&quot;split.count.csv.${uid}&quot;,   &quot;${trainN},${valN},${testN}&quot;)

log.info(&quot;[SPLIT:PRECOMP] ${uid} total=${total} -&gt; train=${trainN}, val=${valN}, test=${testN}&quot;)
</stringProp>
